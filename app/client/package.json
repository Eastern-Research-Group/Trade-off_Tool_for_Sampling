{
  "name": "epa-hsrp-tots-client",
  "version": "1.0.0",
  "private": true,
  "description": "Trade-off Tool for Sampling (TOTS) is JavaScript Web Mapping application to create sampling strategy for chemical and radiological contamination characterization easily accessible to field collection applications via the EPA GeoPlatform.",
  "keywords": [
    "TOTS",
    "Trade-off Tool for Sampling"
  ],
  "repository": "github:USEPA/EPA-HSRP-TOTS",
  "bugs": "https://github.com/USEPA/EPA-HSRP-TOTS/issues",
  "engines": {
    "node": "14.x"
  },
  "main": "src/index.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts -r @cypress/instrument-cra start",
    "lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx,css}'"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@reach/dialog": "0.9.1",
    "@reach/window-size": "0.9.0",
    "@types/file-saver": "2.0.1",
    "@types/react-color": "3.0.4",
    "@types/react-select": "3.0.11",
    "@types/react-table": "7.0.24",
    "@types/uuid": "7.0.2",
    "esri-loader": "2.13.0",
    "excel4node": "1.7.2",
    "file-saver": "2.0.2",
    "react": "16.13.1",
    "react-app-polyfill": "1.0.6",
    "react-color": "2.18.1",
    "react-dom": "16.13.1",
    "react-dropzone": "10.2.2",
    "react-scripts": "3.4.1",
    "react-select": "3.1.0",
    "react-table": "7.6.0",
    "typescript": "3.8.3",
    "universal-cookie": "4.0.3",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@cypress/instrument-cra": "1.4.0",
    "@types/arcgis-js-api": "4.15.0",
    "@types/node": "13.9.5",
    "@types/react": "16.9.27",
    "@types/react-dom": "16.9.5",
    "husky": "4.2.3",
    "lint-staged": "10.1.0",
    "prettier": "2.0.2",
    "stylelint": "13.2.1",
    "stylelint-config-standard": "20.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "npm run lint:css"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
